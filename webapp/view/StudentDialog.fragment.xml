<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog id="editDialog" title="Edit Student">
        <VBox id="_IDGenVBox1">
            <Label id="_IDGenLabel1" text="Student ID" />
            <Input id="_IDGenInput1" value="{dialogModel>/StudentId}" editable="false" />
            
            <Label id="_IDGenLabel2" text="First Name" />
            <Input id="_IDGenInput2" value="{dialogModel>/FirstName}" />
            
            <Label id="_IDGenLabel3" text="Last Name" />
            <Input id="_IDGenInput3" value="{dialogModel>/LastName}" />

            <Label id="_IDGenLabel4" text="Approved" />
            <Input id="_IDGenInput4" value="{dialogModel>/Approved}" />
            
            <Label id="_IDGenLabel5" text="Rejected" />
            <Input id="_IDGenInput5" value="{dialogModel>/Rejected}" />

            <Label id="_IDGenLabel6" text="Category" />
            <Input id="_IDGenInput6" value="{dialogModel>/Category}" />
            
            <Label id="_IDGenLabel7" text="RollNum" />
            <Input id="_IDGenInput7" value="{dialogModel>/RollNum}" />

            <Label id="_IDGenLabel8" text="Branch" />
            <Input id="_IDGenInput8" value="{dialogModel>/Branch}" />
            
            <Label id="_IDGenLabel9" text="Year" />
            <Input id="_IDGenInput9" value="{dialogModel>/Year1}" />

            <Label id="_IDGenLabel10" text="Books Issued" />
            <Input id="_IDGenInput10" value="{dialogModel>/BooksIssued}" />
            
            <Label id="_IDGenLabel11" text="Email Id" />
            <Input id="_IDGenInput11" value="{dialogModel>/EmailId}" />

            
            
            <!-- Add other fields here as needed -->
            
        </VBox>
        <endButton>
            <Button id="_IDGenFrag1" text="Cancel" press=".onCancel" />
        </endButton>
        <beginButton>
            <Button id="_IDGenFrag2" text="Save" press=".onSave" />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>
