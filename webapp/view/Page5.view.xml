<mvc:View xmlns:upload="sap.m.upload" xmlns:unified="sap.ui.unified" controllerName="leftmenu.controller.Page5"
   	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<VBox id="_IDGenVBox1" class="sapUiSmallMargin">
		<f:Form id="FormChange354"
			editable="true">
			<f:title>
				<core:Title id="_IDGenTitle1" text="BRANCHES" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
					labelSpanXL="3"
					labelSpanL="3"
					labelSpanM="3"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="4"
					emptySpanL="4"
					emptySpanM="4"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="_IDGenFormContainer1">
					<f:formElements>
						<f:FormElement id="_IDGenFormElement1" label="ID" >
							<f:fields>
								<Input value="{/Id}" id="name" required="true" type="Number"/>
								<!--Select id="select" items="{ path: '/zstudentsSet' }">
        							<core:Item id="_IDGenItem1" key="{StudentId}" text="{StudentId}" />
    							</Select-->
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement2" label="BRANCH">
							<f:fields>
								<Input value="{/Branch}" id="name1" required="true" type="Text"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
    <FlexBox id="_IDGenFlexBox1"
				height="100px"
				alignItems="Start"
				justifyContent="Center">
				<Button id="_IDGenButton1" text="SAVE" press="onsave" type="Emphasized" class="sapUiSmallMarginEnd" />
				<Button id="_IDGenButton2" text="CANCEL" type="Reject" press="oncancel" class="sapUiSmallMarginEnd" />
				<Button id="_IDGenButton3" text="VIEW REPORT" press="onpreview" type="Emphasized" class="sapUiSmallMarginEnd" />
			</FlexBox>
</mvc:View>
